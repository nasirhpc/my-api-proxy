{
  "version": 2,
  "rewrites": [
    {
      "source": "/",
      "methods": ["GET"],
      "has": [
        {
          "type": "header",
          "key": "accept",
          "value": ".*text/html.*"
        }
      ],
      "destination": "/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "https://script.google.com/a/macros/hpadminbd.com/s/AKfycby9pxeBMRwNSQP3EqZr3q3gaHtcMOEqcBFm2gZZOIWRn7fIIIxZRor26BIY29OFylh_iA/exec"
    }
  ]
}
